.data

.text

main:
move $fp $sp
sw $ra, 0($sp)
addiu $sp, $sp, -4
li $a0, 4
sw $a0, 0($sp)
addiu $sp, $sp, -4
li $a0, 5
lw $t1, 4($sp)
addiu $sp, $sp, 4
add $a0, $t1, $a0
sw $a0, 0($sp)
addiu $sp, $sp, -4
jal output
end_main:
lw $ra, 4($sp)
addiu $sp, $sp, 8
jr $ra


.text
.globl __crt_out_of_bounds
.globl output
.globl input

__crt_out_of_bounds:
li $v0, 10 # exit
syscall

output:
lw $a0, 4($sp)
li $v0, 1
syscall
li $v0, 11
li $a0, 0x0a
syscall
addiu $sp, $sp, 4
li $a0, 0
j $ra

input:
li $v0, 5
syscall
jr $ra

